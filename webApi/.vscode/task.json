{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "command": "dotnet",
        "type": "shell",
        "args": ["build"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "reveal": "always",
          "panel": "new"
        }
      },
      {
        "label": "generate-xml-doc",
        "command": "dotnet",
        "type": "shell",
        "args": [
          "build",
          "/property:GenerateDocumentationFile=true"
        ],
        "group": "build",
        "dependsOn": "build"
      }
    ]
  }
  